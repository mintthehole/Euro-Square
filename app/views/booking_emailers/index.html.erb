<h1>Listing booking_emailers</h1>

<table>
  <tr>
    <th>Booking</th>
    <th>User</th>
    <th>Emailer</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @booking_emailers.each do |booking_emailer| %>
  <tr>
    <td><%= booking_emailer.booking %></td>
    <td><%= booking_emailer.user %></td>
    <td><%= booking_emailer.emailer %></td>
    <td><%= booking_emailer.state %></td>
    <td><%= link_to 'Show', booking_emailer %></td>
    <td><%= link_to 'Edit', edit_booking_emailer_path(booking_emailer) %></td>
    <td><%= link_to 'Destroy', booking_emailer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

